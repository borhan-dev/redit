{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "rules": {
    "indent": "off",
    "@typescript-eslint/explicit-function-return-type": "off",
    "prefer-const": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}